<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/goodsclass_nav"
    app:startDestination="@id/gallery">

    <fragment
        android:id="@+id/gallery"
        android:name="com.example.cimoshop.ui.goodsclass.gallery.Gallery"
        android:label="gallery_fragment"
        tools:layout="@layout/gallery_fragment" >
        <action
            android:id="@+id/action_gallery_to_galleryDetail"
            app:destination="@id/galleryDetail"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/galleryDetail"
        android:name="com.example.cimoshop.ui.goodsclass.gallery.GalleryDetail"
        android:label="gallery_detail_fragment"
        tools:layout="@layout/gallery_detail_fragment" >
        <action
            android:id="@+id/action_galleryDetail_to_gallery"
            app:destination="@id/gallery" />
    </fragment>
</navigation>